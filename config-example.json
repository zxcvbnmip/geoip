      "type": "clashRuleSetClassical",
      "action": "output",
      "args": {
        "wantedList": ["cn", "private", "test"],
        "onlyIPType": "ipv4"
      }
    },
    {
      "type": "clashRuleSet",
      "action": "output",
      "args": {
        "wantedList": ["cn", "private", "test"],
        "onlyIPType": "ipv6"
      }
