        "onlyIPType": "ipv4"
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "add",
      "args": {
        "uri": "https://raw.githubusercontent.com/Loyalsoldier/geoip/release/Country.mmdb"
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "remove",
