    {
      "type": "surgeRuleSet",
      "action": "output",
      "args": {
        "wantedList": ["cn", "private", "test"],
        "onlyIPType": "ipv4"
      }
    },
    {
      "type": "surgeRuleSet",
      "action": "output",
      "args": {
        "addSuffixInLine": ",no-resolve"
      }
