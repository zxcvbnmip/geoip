        "uri": "./geoip.dat",
        "wantedList": ["cn", "us"],
        "onlyIPType": "ipv4"
      }
    },
    {
      "type": "v2rayGeoIPDat",
      "action": "add",
      "args": {
        "uri": "https://raw.githubusercontent.com/Loyalsoldier/geoip/release/geoip.dat",
        "onlyIPType": "ipv6"
      }
    },
    {
      "type": "maxmindGeoLite2CountryCSV",
      "action": "add",
      "args": {
        "country": "./geolite2/GeoLite2-Country-Locations-en.csv",
