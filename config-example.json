        "onlyIPType": "ipv6"
      }
    },
    {
      "type": "singboxSRS",
      "action": "add",
      "args": {
        "name": "cn",
        "uri": "https://raw.githubusercontent.com/Loyalsoldier/geoip/release/srs/cn.srs"
      }
    },
    {
      "type": "singboxSRS",
      "action": "add",
      "args": {
        "name": "cn",
        "uri": "./srs/cn.srs",
        "onlyIPType": "ipv4"
      }
    },
    {
      "type": "singboxSRS",
      "action": "add",
      "args": {
        "inputDir": "./srs",
        "onlyIPType": "ipv6"
      }
    },
    {
      "type": "cutter",
      "action": "remove",
      "args": {
        "wantedList": ["cn", "us"],
