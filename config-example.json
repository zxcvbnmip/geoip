        "wantedList": ["cn", "private"]
      }
    },
    {
      "type": "v2rayGeoIPDat",
      "action": "output",
      "args": {
        "outputDir": "./publish",
        "oneFilePerList": true,
        "wantedList": ["cn", "private", "test"],
        "onlyIPType": "ipv4"
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "output"
    },
    {
      "type": "maxmindMMDB",
      "action": "output",
      "args": {
        "outputName": "Country.mmdb",
        "overwriteList": ["cn", "private"]
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "output",
      "args": {
        "outputName": "country-only-cn-private.mmdb",
        "wantedList": ["cn", "private"]
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "output",
      "args": {
        "outputDir": "./publish",
