        "ipv6": "./geolite2/GeoLite2-Country-Blocks-IPv6.csv",
        "wantedList": ["cn", "us"]
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "add"
    },
    {
      "type": "maxmindMMDB",
      "action": "add",
      "args": {
        "uri": "./path/to/your/mmdb/file",
